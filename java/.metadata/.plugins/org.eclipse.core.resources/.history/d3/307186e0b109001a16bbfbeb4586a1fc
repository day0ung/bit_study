package varialble;

public class mainClass {
	public static void main(String[] args) {
		/*
		  1.소스코드 
		  
		  2.compile(문자코드-> 기계어(2진수)) - library참조파일(*.jar)
		  
		  3.build == .exe(cute) 실행파일
		  
		  4.실행 
		 */
		
		/*
		 	정수(==상수)
		 	2진수 	0,1
		 	8진수		0~7 -> 010 -> 8
		 	10진수
		 	16진수
		  		  
		 */
		int n = 0X80;
		System.out.printf("%x",n);
	}


}
