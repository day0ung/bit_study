package work1;

import java.util.Scanner;

public class mainClass {

	public static void main(String[] args) {
		//편의점
		/*
		 	입력
		  	지불금액: 3230원
		  	money: 10000원
		  	
		  	거스름돈: 
		  	5000원 -> ?장
		  	1000원 -> ?장
		  	500원 -> ?개
		  	100원 -> ?개
		  	50원 -> ?개
		  	10원 -> ?개
		  	
		  	결과: ????원
		 */
		Scanner scan  = new Scanner(System.in);
		int money = 10000;
		int a = scan.nextInt();
		System.out.println(money - a+"원");
		System.out.println("거스름돈: ");
		

	}

}
