drop table ORDER_TABLE
CASCADE CONSTRAINTS;

CREATE TABLE ORDER_TABLE(
   SEQ NUMBER(8) PRIMARY KEY,
   ID VARCHAR2(30) NOT NULL,
   BEVERAGE VARCHAR2(40) NOT NULL,
   SYRUP VARCHAR2(9),
   C_SIZE VARCHAR2(10),
   SHOT VARCHAR2(8),
   WHEEPING VARCHAR2(8),
   CUP NUMBER(8) ,
   PRICE NUMBER(8),
   WDATE DATE
);

create SEQUENCE SEQ_COFFEE_ORDERED
START WITH 100
INCREMENT BY 1;

ALTER TABLE ORDER_TABLE
ADD CONSTRAINT FK_COFFEE_ID FOREIGN KEY(ID)
REFERENCES MEMBER(ID);
