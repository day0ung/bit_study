package game;



import java.awt.Label;

import javax.swing.JFrame;

public class GbbGame extends JFrame {
	int win = 0;
	int lose = 0;
	int draw = 0;
	public GbbGame() {
		super("가위바위보 게임 과제");
		
		Label label = new Label("승"+win + "패" + lose + "무" + draw);
		add(label);
		
		
		setSize(800, 600);
		setLocation(100, 0);
		setVisible(true);
		
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
	}
	
}
